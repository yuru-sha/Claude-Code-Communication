{
  "name": "claude-code-communication",
  "version": "1.0.0",
  "description": "複数の AI が協力して働く、まるで会社のような開発システムです",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:server": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/backend/server.ts",
    "build": "vite build",
    "build:server": "tsc src/backend/server.ts --outDir dist/backend --target es2020 --module commonjs --esModuleInterop",
    "start": "NODE_ENV=production node dist/backend/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
